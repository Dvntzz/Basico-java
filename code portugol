programa
{
	inclua biblioteca Util
	inteiro contador = 11, senha, senha1 = 23456
	
	funcao inicio()
	{
		escreva("digite a senha da bomba: ")
		leia(senha)
		limpa()
		
		se(senha == senha1){
			enquanto(contador > 0){
		     contador = (contador - 1)
			escreva("Detonação em ", contador)
			Util.aguarde(1000)
			limpa()}
			
			}
			senao se(senha != senha1){
				escreva("senha incorreta")
			}
			escreva("BOOOOM")
			
	}
	
}
